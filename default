default vesamenu.c32
prompt 0
timeout 300
ONTIMEOUT local

MENU TITLE PXE Boot Menu

LABEL local
	MENU LABEL ^Boot local hard drive
        LOCALBOOT 0

LABEL Install db001srv
        MENU LABEL Install db001srv
        kernel OracleLinux_7_3/vmlinuz
        append ksdevice=eth0 console=tty0 load_ramdisk=1 initrd=OracleLinux_7_3/initrd.img inst.ks=http://10.0.0.250/OracleLinux_7_3/OL73_db001srv.ks inst.repo=http://10.0.0.250/OracleLinux_7_3/ISO/

